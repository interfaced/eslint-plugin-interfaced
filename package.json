{
  "name": "eslint-plugin-interfaced",
  "version": "1.5.0",
  "description": "ESLint plugin with rules for the approaches adopted by Interfaced company",
  "license": "MIT",
  "author": "Interfaced (http://interfaced.tv)",
  "homepage": "https://github.com/interfaced/eslint-plugin-interfaced",
  "contributors": [
    "Kirill Dronkin <kirilldronkin@gmail.com>",
    "Goryaynov Igor <ygreknomean@gmail.com>"
  ],
  "engines": {
    "node": ">=6",
    "npm": ">=5"
  },
  "scripts": {
    "lint": "eslint lib test",
    "test": "npm run test.eslint && npm run test.unit",
    "test.eslint": "mocha test/eslint",
    "test.unit": "mocha test/unit"
  },
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "index.js",
    "lib",
    "docs"
  ],
  "keywords": [
    "lint",
    "eslint",
    "plugin",
    "interfaced",
    "lines",
    "whitespace",
    "order",
    "jsdoc",
    "const",
    "event",
    "enum",
    "typedef",
    "typecast"
  ],
  "peerDependencies": {
    "eslint": "^5.0.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "eslint": "5.0.1",
    "eslint-config-interfaced": "0.1.0",
    "mocha": "5.2.0"
  },
  "dependencies": {
    "doctrine": "github:interfaced/doctrine#range-fixes",
    "esutils": "2.0.2"
  }
}
